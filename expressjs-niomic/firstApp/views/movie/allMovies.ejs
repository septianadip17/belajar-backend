<% include ../header %>
<a href="movies/create" class="btn btn-success">Add Movies</a>
<% for (let data of ListMovies) { %>
<div class="card">
  <div class="card-header">
    <%= data.name %>
  </div>
  <div class="card-body">
    <h5 class="card-title">Tanggal Tayang: <%= data.released_on %></h5>
    <a href="#" class="btn btn-primary">Edit</a>
    <a href="#" class="btn btn-danger">Delete</a>
  </div>
</div>
<% } %>